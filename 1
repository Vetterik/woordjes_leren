import random
latijn = {'deus':'god', 'est': 'het is', 'pater': 'vader','patrem':'vader', 'et':'en,ook', 'dominus':'heer,heerser', 'patrem':'vader', 'dea':'godin'
          , 'regina':'koningin', 'uxor':' vrouw, echtgenote', 'uxorum':'vrouw,echtgenote','mater': 'moeder', 'matrem':'moeder','filius':'zoon','filia':'dochter','non':'niet ','sed':'maar','quoque':'ook','soror':'zuster','sororum':'zuster'
}
latijn_list = list(latijn.values())
print (latijn_list)
rand_item = random.choice(latijn_list)
print(rand_item)
latijn_list.remove(rand_item)
print('vul een woord in: (leeglaten als je wilt stoppen)')
woord = input()    
if woord in latijn:
  print (woord + ' = ' + latijn[woord])
else:
  print('deze ken ik niet')
#############
